
@using WebApp.Models.Helpers;

<script src="~/Scripts/Views/Membresia.js"></script>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main>

    <div class="row">

        <div class="col-md-12 table-card">
              <table id="tblMembresia">
                  <tr><th>Nombre</th><th>Monto Mensual</th><th>Comisión por Transacción</th><th>Número de Días</th></tr>
                  <tr>
                      <td><label id="lblNombre"></label></td>
                      <td><label id="lblMonto"></label></td>
                      <td><label  id="lblComision"></label></td>
                      <td><lablel id="lblNum"></lablel></td>
                  </tr>

              </table>
 
            <div class="card border-secondary" style="margin-top: 50px;">

                <div class="row">
              
                    <div class="col-md-12">
                        @Html.CtrlButton(viewName: "Membresia", id: "btnAceptar", label: "Aceptar", onClickFunction: "Aceptar", buttonType: "success")
                        @Html.CtrlButton(viewName: "Membresia", id: "btnRechazar", label: "Rechazar", onClickFunction: "Rechazar", buttonType: "danger")
  
                    </div>
                </div>

            </div>
        </div>

    </div>

</main>
